<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.limengxiang.breeze.model.dao.AppCredentialMapper">
    <select id="loadCredential" resultType="com.limengxiang.breeze.http.auth.AuthCredential">
        SELECT token,privilege FROM brz_app_credential WHERE app_id = #{appId} AND status=0 LIMIT 1
    </select>
</mapper>
